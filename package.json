{
  "name": "@pitcher/vue-project-template",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-update": "vue-cli-service test:unit --updateSnapshot",
    "test:unit-coverage": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build; cd dist; kudu push -f HERECOMESMYFILEID;echo 'deployed'",
    "ios:watch": "DESTINATION=$(FILE_ID=HERECOMESMYFILEID node utils/find-active-sim-app-folder.js) && NODE_ENV=development vue-cli-service build --watch --mode development --dest \"$DESTINATION\"",
    "android:logcat": "~/Library/Android/sdk/platform-tools/adb -s emulator-5554 logcat > log.txt",
    "android:pull-db": "~/Library/Android/sdk/platform-tools/adb -s emulator-5554 root;~/Library/Android/sdk/platform-tools/adb -s emulator-5554 root; pull /data/user/0/com.pitcher.android/databases/",
    "devtools": "vue-devtools",
    "gettext:compile": "npx config-gettext-compile",
    "gettext:extract": "npx config-gettext-extract",
    "gettext:translate": "export GOOGLE_APPLICATION_CREDENTIALS=google.json;npx config-gettext-translate",
    "pull:config": "kudu pull -f HERECOMESMYCONFIGFILEID -p pitcherconfig.json",
    "ti-web:collect-ios": "npm run pull:config && ti-web-collect-ios",
    "ti-web:serve": "ti-web"
  },
  "dependencies": {
    "@pitcher/core": "*",
    "@pitcher/i18n": "*",
    "@pitcher/pitcherify": "*",
    "@sentry/integrations": "^6.0.0",
    "@sentry/tracing": "^6.0.0",
    "@sentry/vue": "^6.0.0",
    "@vue/composition-api": "^1.0.0-rc.1",
    "core-js": "^3.8.0",
    "fastclick": "^1.0.6",
    "roboto-fontface": "*",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "vuetify": "^2.4.2"
  },
  "devDependencies": {
    "@google-cloud/translate": "^6.0.0",
    "@pitcher/easygettext": "^2.14.0",
    "@pitcher/eslint-config": "^1.2.1",
    "@pitcher/ti-web": "^1.5.0",
    "@pitcher/vue-cli-plugin-pitcherify": "~0.1.3",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.8.0",
    "gettext-parser": "^4.0.3",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "sass": "~1.32.0",
    "sass-loader": "~10.2.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vuetify-loader": "^1.3.0"
  }
}
